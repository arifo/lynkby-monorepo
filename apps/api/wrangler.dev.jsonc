/**
 * Development environment configuration for @lynkby/api
 * This extends the base configuration with development-specific settings
 */
{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "api-dev",
  "main": "src/index.ts",
  "compatibility_date": "2025-09-02",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "observability": {
    "enabled": true
  },
  /**
	 * Hyperdrive configuration for development
	 */
  "hyperdrive": [
    {
      "binding": "HYPERDRIVE",
      "id": "3c39294851eb43a287e0de9155c76454",
    }
  ],
  "kv_namespaces": [
    {
      "binding": "KV_CACHE",
      "id": "b6dd729a9b274cb0ac03732266503f56",
      "preview_id": "b6dd729a9b274cb0ac03732266503f56"
    }
  ],
  /**
	 * Development Environment Variables (Non-sensitive)
	 */
  "vars": {
    "NODE_ENV": "development",
    "APP_BASE": "http://localhost:8788",
    "JWT_EXPIRES_IN": "7d",
    "EMAIL_FROM": "dev@lynkby.com",
    "SUPPORT_EMAIL": "dev-support@lynkby.com",
    "APP_NAME": "Lynkby Dev"
  }
  /**
	 * Development Secrets (Set with `wrangler secret put --env development`)
	 * 
	 * Required secrets for development:
	 * - JWT_SECRET: Development JWT secret (min 32 chars)
	 * - REVALIDATE_SECRET: Development revalidation secret
	 * - SENTRY_DSN: Development Sentry DSN (optional)
	 * - DATABASE_URL: Development database URL
	 * - DIRECT_URL: Development direct database URL
	 * - STRIPE_SECRET_KEY: Stripe test secret key
	 * - STRIPE_WEBHOOK_SECRET: Stripe test webhook secret
	 * - TIKTOK_CLIENT_KEY: TikTok test client key
	 * - TIKTOK_CLIENT_SECRET: TikTok test client secret
	 * - RESEND_API_KEY: Resend test API key
	 */
}