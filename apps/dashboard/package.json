{
  "name": "@lynkby/dashboard",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "deploy": "pnpm build && wrangler deploy --config wrangler.toml",
    "deploy:dev": "pnpm build && wrangler deploy --config wrangler.toml --env development",
    "deploy:prod": "pnpm build && wrangler deploy --config wrangler.toml --env production",
    "prisma:migrate": "prisma migrate dev --schema ../../packages/server/prisma/schema.prisma",
    "prisma:generate": "prisma generate --schema ../../packages/server/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema ../../packages/server/prisma/schema.prisma",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "cookie": "^1.0.2",
    "hono": "^4.5.7",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250115.0",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "prisma": "6.14.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.5",
    "typescript": "^5.5.4",
    "wrangler": "^4.32.0"
  }
}